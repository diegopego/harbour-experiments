CLASS USAddress

   DATA Street

   DATA City
   METHOD New( Street, City )

ENDCLASS

METHOD New( Street, City ) CLASS USAddress

   ::Street := Street
   ::City := City

   RETURN Self

CLASS UKAddress

   DATA Street

   DATA Town
   DATA PostCode
   METHOD New( Street, Town, PostCode )

ENDCLASS

METHOD New( Street, Town, PostCode ) CLASS UKAddress

   ::Street := Street
   ::Town := Town
   ::PostCode := PostCode

   RETURN Self
